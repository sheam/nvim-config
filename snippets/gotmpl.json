{
	"html tag": {
		"prefix": "tag",
		"description": "open and close html tag",
		"body": [
			"<${1} ${2}>",
			"\t${3}",
			"</${1}>"
		]
	},
	"end directive": {
		"prefix": "end",
		"description": "end a template directive",
		"body": [
			"{{ end }}"
		]
	},
	"define template directive": {
		"prefix": "define",
		"description": "define a template directive",
		"body": [
			"{{ define \"${1}\" }}",
			"\t${2}",
			"{{ end }}"
		]
	},
	"use a template": {
		"prefix": "temp",
		"description": "use a template directive",
		"body": [
			"{{ template \"${1}\" .${2} }}",
			"${3}"
		]
	},
